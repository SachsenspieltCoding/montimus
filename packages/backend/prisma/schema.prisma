datasource db {
    provider = "sqlite"
    url      = "file:../../../data/db.sqlite"
}

generator client {
    provider = "prisma-client-js"
}

model Monitor {
    id          Int      @id @default(autoincrement())
    name        String
    description String?
    url         String
    interval    Int
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt
}